{
  "Routing": { "Type": "dhtserver" },
  "Addresses": {
    "Swarm": [
      "/ip4/0.0.0.0/tcp/4001",
      "/ip6/::/tcp/4001",
      "/ip4/0.0.0.0/udp/4001/quic",
      "/ip6/::/udp/4001/quic"
    ]
  },
  "Swarm": {
    "RelayService": { "Enabled": true },
    "ConnMgr": { "LowWater": 200, "HighWater": 400, "GracePeriod": "2m" },
    "EnableAutoRelay": false
  },
  "Pubsub": { "Enabled": true, "Router": "gossipsub" },
  "Reprovider": { "Strategy": "pinned", "Interval": "6h" }
}
