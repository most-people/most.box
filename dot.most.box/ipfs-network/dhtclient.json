{
  "Routing": { "Type": "dhtclient" },
  "Addresses": {
    "Swarm": [
      "/ip4/0.0.0.0/tcp/4001",
      "/ip6/::/tcp/4001",
      "/ip4/0.0.0.0/udp/4001/quic",
      "/ip6/::/udp/4001/quic"
    ]
  },
  "Swarm": {
    "RelayClient": { "Enabled": true },
    "EnableAutoRelay": true,
    "ConnMgr": { "LowWater": 100, "HighWater": 200, "GracePeriod": "2m" }
  },
  "Pubsub": { "Enabled": true, "Router": "gossipsub" },
  "Reprovider": { "Strategy": "pinned", "Interval": "12h" }
}
